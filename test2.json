            "schema": {
              "type": "integer",
              "minimum": 0,
              "maximum": 10000,
              "default": 10
            }
          },
          {
            "in": "query",
            "name": "offset",
            "description": "The offset of page to return.",
            "required": false,
            "example": 0,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "default": 0
